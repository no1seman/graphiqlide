{
  "name": "graphiqlide",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tarantool.io/ui-kit": "0.21.0",
    "emotion": "^10.0.27",
    "graphiql": "1.0.3",
    "graphiql-explorer": "^0.6.2",
    "graphql": "^15.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/preset-flow": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@babel/runtime": "7.11.2",
    "@babel/traverse": "7.11.0",
    "@babel/types": "7.11.0",
    "@prettier/plugin-xml": "^0.11.0",
    "@tarantool.io/lua-bundler-webpack-plugin": "1.0.1",
    "array-move": "^3.0.1",
    "autoprefixer": "9.8.6",
    "axios": "^0.19.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "babel-plugin-emotion": "10.0.33",
    "babel-preset-react-app": "9.1.2",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "chalk": "4.1.0",
    "css-loader": "4.2.1",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "7.7.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0",
    "file-loader": "6.0.0",
    "file-saver": "2.0.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "4.3.0",
    "jest": "26.4.0",
    "less-loader": "^6.2.0",
    "lodash": "^4.17.20",
    "mime-types": "^2.1.27",
    "normalize-url": "5.1.0",
    "object-assign": "4.1.1",
    "plop": "2.7.4",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "prettier": "^2.0.5",
    "promise": "8.1.0",
    "prop-types": "^15.7.2",
    "raf": "3.4.1",
    "react-dev-utils": "^10.2.1",
    "resolve": "1.17.0",
    "source-map-explorer": "^2.4.2",
    "style-loader": "1.2.1",
    "svg-sprite-loader": "5.0.0",
    "svgo": "1.3.2",
    "svgo-loader": "2.2.1",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "4.1.0",
    "webpack": "4.44.1",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-dev-server": "3.11.0",
    "webpack-manifest-plugin": "2.2.0",
    "whatwg-fetch": "3.4.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/graphqlide/main.*",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js test --env=jsdom --watch",
    "test_once": "node ./scripts/test.js test --env=jsdom",
    "lint": "eslint ./src",
    "lint-fix": "eslint --fix ./src",
    "config": "plop",
    "build-rock": "tarantoolctl rocks build && tarantoolctl rocks pack graphqlide 0.0.1-1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-flow",
      "react-app"
    ],
    "plugins": [
      "emotion"
    ]
  }
}
